package dbDiagram.components;

import dbDiagram.components.dataType.AbstractDataType;

public class ForeignKey  extends Field {

	private String fkName;
	

	public ForeignKey(String name, String fkName, AbstractDataType type)
	{	super(name, type);
		this.fkName = fkName;
	}
	
	public ForeignKey(Field f, String fkName) {
		super(f.getName(), f.getType());
		this.fkName = fkName;
	}
	
	public String getFkName() {
		return fkName;
	}
	
	public void setFkName(String fkName) {
		this.fkName = fkName;
	}

}
